<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.winbee.vasantvalleyschool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:windowSoftInputMode="adjustPan">
        <activity android:name=".VimeoActivity"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".VimeoPlayerActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".ResetPassWordActivity" />
        <activity android:name=".AboutUsActivity" />
        <activity android:name=".ViewResultActivity" />
        <activity android:name=".InstructionsActivity" />
        <activity android:name=".OnlineQuestionActivity" />
        <activity android:name=".AskedNewQuestionActivity" />
        <activity android:name=".AskedQuestionActivity" />
        <activity android:name=".AskedSolutionActivity" />
        <activity
            android:name=".MyCourseSubjectActivity"
            android:label="Subject"
            android:parentActivityName=".Ui.Home" />
        <activity android:name=".DriveVideoPlayerActivity" />
        <activity android:name=".GecSemesterTopicActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".GecYouTubeActivity" />
        <activity android:name=".Login" />
        <activity android:name=".SignUp" />
        <activity android:name=".Ui.Home" />
        <activity android:name=".OtpVerficationActivity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".Utils.MyMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>